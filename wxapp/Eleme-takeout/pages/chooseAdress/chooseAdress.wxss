/* pages/chooseAdress/chooseAdress.wxss */
.page{
   background-color: #eee;
   color: #888888;
   min-height: 100vh;
}
.navigation{
   width: 100%;
   height: 100rpx;
   position: fixed;
   top: 0;
   background-color: #0097ff;
   font-size: 32rpx;
   text-align: left;
   padding-top: 36rpx;
   display: flex;
   align-items: center;
   padding-left: 20rpx;
}
.navigation .arrow{
   width: 20rpx;
   height: 20rpx;
   border: 4rpx solid #fff;
   border-color: transparent transparent #fff #fff;
   transform: rotate(45deg);
}
.navigation .nav-name{
   flex: 1;
   color: #ffffff;
   font: bold;
}
.chooseAd {
   display: flex;
   align-items: center;
   background-color: #0097ff;
   position: absolute;
   top: 136rpx;
   width: 100%;
   font-size: 26rpx;
}
.chooseAd text{
   color: #ffffff;
   padding: 20rpx;
}
.chooseAd .arrow2{
   width: 0;
   height: 0;
   border: 10rpx solid white;
   border-color: white transparent transparent;
   margin-top: 5rpx; 
   margin-left: 10rpx;
}
.chooseAd input{
   flex: 1;
   height: 50rpx;
   background-color: #fff;
   border-radius: 6rpx;
   text-align: center;
   color: #888888;
   margin: 0 10rpx;
}
.mian-wrapper {
   color: #000000;
   position: absolute;
   top: 214rpx;
   width: 100%;
   font-size: 26rpx;
   background-color: #eee;
}
.detail {
   line-height: 26rpx;
   height: 26rpx;
   padding: 26rpx;
   background-color: #fff;
   color: #0097ff;
   display: flex;
}
.detail .right {
   flex: 1;
   text-align: right;
}
.split {
   line-height: 26rpx;
   height: 26rpx;
   padding: 16rpx 26rpx;
   color: #555555;
}
.receiptAd {
   background-color: #fff;
   padding: 20rpx;
   font-size: 22rpx;
}
.place {
   padding: 4rpx 5rpx 0 5rpx;
   color: #0097ff;
   border: 1px solid #0097ff;
}
.userMsg{
   margin-top: 10rpx;
}
.detail-place {
   font-weight: 500;
   overflow: hidden;
   padding-left: 10rpx;
}

.userMsg{
   color: #555555;
}
.bottom {
   width: 100%;
   text-align: center;
   padding: 25rpx 0;
   font-size: 30rpx;
   position: fixed;
   bottom: 0;
   background-color: #fff;
   color: #0097ff;
}
